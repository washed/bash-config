# Enable tab completion
source ~/bin/git-completion.bash

PATH=$PATH:C:/MinGW/msys/1.0/bin

# Standard aliases
alias npp="C:/Program\ Files\ \(x86\)/Notepad++/notepad++.exe -multiInst -notabbar -nosession -noPlugin"
alias cff="~/bin/git-cff.sh"
alias gnf="~/bin/git-helpers.sh nf"
alias gmf="~/bin/git-helpers.sh mf"
alias gnb="~/bin/git-helpers.sh nb"
alias gmb="~/bin/git-helpers.sh mb"
alias gnhf="~/bin/git-helpers.sh nhf"
alias ginit="~/bin/git-helpers.sh init"

# Source user specifics
source ~/.user_bash_profile